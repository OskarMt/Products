>\r\n          <div class=\"priceRetail\">{{priceRetail}}</div>\r\n\r\n          <span @click=\"useMeter()\">За {{priceUnitAlt}}</span>\r\n          <span @click=\"usePack()\">За {{priceUnit}}</span>\r\n\r\n\r\n          <div class=\"counter\">{{counter}}</div>\r\n          <button @click=\"addCount()\">add</button>\r\n          <button @click=\"removeCount()\">remove</button>\r\n\r\n\r\n          <div>\r\n          <a href=\"#\" v-bind:data-product-id=\"productId\">Купить</a>\r\n          </div>\r\n  </div>\r\n\r\n\r\n</template>\r\n\r\n\r\n<script>\r\nimport Products from '../../products.json'\r\nexport default{\r\n    props:{\r\n       productCounter: Number,\r\n    },\r\n    data(){\r\n        return {\r\n\r\n\r\n                title: Products[this.productCounter].title,\r\n                description: Products[this.productCounter].description,\r\n                productRequired:Products[this.productCounter].assocProducts,\r\n                productImg:Products[this.productCounter].primaryImageUrl,\r\n\r\n                priceGold: Products[this.productCounter].priceGoldAlt,\r\n                priceRetail:Products[this.productCounter].priceRetailAlt,\r\n                priceUnit:Products[this.productCounter].unit,\r\n                priceUnitAlt:Products[this.productCounter].unitAlt,\r\n\r\n                productId:Products[this.productCounter].productId,\r\n                productCode:Products[this.productCounter].code,\r\n                weight:Products[this.productCounter].weight,\r\n\r\n\r\n                counter: 1,\r\n\r\n            }\r\n    },\r\n  methods:{\r\n      addCount(){\r\n        this.counter++\r\n      },\r\n       removeCount(){\r\n         if(this.counter > 1){\r\n           this.counter--\r\n         }\r\n      },\r\n      useMeter(){\r\n          this.priceGold = Products[0].priceGoldAlt;\r\n          this.priceRetail = Products[0].priceRetailAlt;\r\n      },\r\n      usePack(){\r\n          this.priceGold = Products[0].priceGold;\r\n          this.priceRetail = Products[0].priceRetail;\r\n      },\r\n      imageModificator(image){\r\n          let imageName = image.split('.');\r\n          imageName[imageName.length - 2] = imageName[imageName.length - 2] + '_220x220_1'\r\n          return imageName.join('.')\r\n      }\r\n  }\r\n\r\n}\r\n\r\n</script>\r\n\r\n\r\n\r\n\r\n<style>\r\n\r\n    @import '/src/style/css/style.css';\r\n\r\n</style>\r\n"]}]}